{# src/web3tc/EchangeBundle/Resources/views/Echange/formulaire.html.twig #}

{% extends "::base.html.twig" %}


{% block body %}
<h3>Selectionnez vos pays</h3>
    <div class="well">
        <form method="post" action="{{ path('_carte') }}">
            <select name="pays" multiple class="col-lg-12" > 
                {% for p in pays %}
                        <option value="{{ p.nom }}">{{ p.nom }}</option>
                {% endfor %}
            </select>
            <input type="submit" class="btn btn-primary" />
        </form>
    </div>

    
{% endblock %}

