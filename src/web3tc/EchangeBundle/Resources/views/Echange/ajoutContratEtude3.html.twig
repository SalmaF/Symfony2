{% extends "::base.html.twig" %}

{% block title %}web3tcEchangeBundle:Admin:pays{% endblock %}

{% block body %}
<h1>Ajouter un pays</h1>

        <div class="progress progress-striped active">
        <div class="progress-bar"  role="progressbar" aria-valuenow="30" aria-valuemin="0" aria-valuemax="100" style="width: 70%">
            <span class="sr-only">70% Complete</span>
        </div>
       </div>

   <div class="jumbotron">
        <form class="form-in-line" method="post" role="form" {{ form_enctype(form) }}>
 
{{ form_rest(form) }}

        <input type="submit" class="btn btn-primary" />
        </form>
   </div>
        {% endblock %}
